<%= render 'quizzes/menu', :quiz => quiz, :course => course,
                           :active => 'result' %>

<div class="page-header">
  <h5><%= quiz.name %></h5>
</div>

<div class="alert">
  <%= t('quizzes.quiz_completed') %>
</div>

<table class="table table-striped table-hover">
  <tbody>
    <tr>
      <td>Rank</td>
      <td><%= quiz.rank(current_user.id) %>/<%= quiz.scores.map(&:user_id).count %></td>
    </tr>
    <tr>
      <td>Score</td>
      <td><%= quiz.score(current_user.id).percent %>%</td>
    </tr>
  </tbody>
</table>
